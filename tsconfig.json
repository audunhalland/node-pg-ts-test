{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "server_dist",
    "baseUrl": "./",
    "allowJs": false,
    "paths": {
      "db-migrate": ["src/typedefs/db-migrate"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
